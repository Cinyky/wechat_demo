.container { width: 100%; height: 100%; }
.header { width: 100%; height: auto; }
.fixed { width: 95%; height: 80rpx; line-height: 80rpx; font-size: 24rpx; background: #fff; margin: 0 auto; }
.fixed>wx-view:nth-child(3){ width: 60rpx; height: 100%; right: 0; top: 0; }
.fixed>wx-view:nth-child(4){ width: 60rpx; height: 100%; right: 60rpx; top: 0; }
.fixed>wx-view wx-image{ width: 50rpx; height: 50rpx; }
.header>wx-view:nth-child(3) { margin-bottom: 20rpx; }
.classification{ width: 100%; height: auto; background: #fff; position: fixed; border-top: 1rpx solid #e5e5e5; bottom: 0; z-index: 50000; }
.classification>wx-view{ width: 100%; font-size: 30rpx; line-height: 80rpx; text-align: center; border-bottom: 1rpx solid #e5e5e5; }
.classification>wx-view:nth-child(1){ height: 80rpx; background: #ED414A; color: #fff; }
.classification>wx-view:nth-child(1){ height: auto; }
.classification>wx-view wx-view{ border-bottom: 1rpx solid #e5e5e5; }
.classification>wx-view wx-view:last-child{ border: none; }
.classification>wx-view:nth-child(1){ height: 80rpx; }
.header>wx-view:nth-child(1) { margin-top: 20rpx; }
.header>wx-view wx-text { width: 20%; margin-left: 5%; height: 100%; display: inline-block; }
.header>wx-view wx-input { width: 75%; height: 100%; display: inline-block; vertical-align: top; }
.label { width: 95%; height: auto; background: #fff; border-top: 1rpx solid #e5e5e5; margin: 0 auto; padding: 0 0 20rpx 0; }
.checkbox:nth-child(1) { margin-left: 5%; }
.checkbox:nth-child(4) { margin-left: 5%; }
.checkbox { font-size: 28rpx; zoom: 70%; width: 30%; margin-top: 40rpx; display: inline-block; }
.page { width: 95%; height: auto; margin: 20rpx auto; }
.goods_details{ width: 95%; height: auto; min-height: 200rpx; background: #fff; margin-left: 2.5%; margin-bottom: 20rpx; }
.goods_details>wx-view{ width: 95%; height: 80rpx; margin-left:5%; line-height: 80rpx; font-size: 26rpx; }
.goods_details>wx-textarea{ width: 90%; height: 120rpx; margin-left: 5%; font-size: 26rpx; line-height: 40rpx; }
.fix { width: 100%; height: 80rpx; line-height: 80rpx; font-size: 24rpx; background: #fff; text-indent: 5%; border-bottom: 1rpx solid #e5e5e5; }
.upload{ width: 100%; height: auto; background: #fff; padding-bottom: 30rpx; }
.upload>wx-view:nth-child(1){ margin-left: 5%; }
.upload>wx-view:nth-child(5){ margin-left: 5%; }
.upload>wx-view:nth-child(9){ margin-left: 5%; }
.upload>wx-view{ display: inline-block; vertical-align: top; height: 170rpx; margin-top: 30rpx; width: 22.5%; }
.upload>wx-view wx-image{ width: 95%; height: 100%; left: 0; }
.upload_img{ border: 1rpx solid #e5e5e5; -webkit-box-sizing: border-box; box-sizing: border-box; }
.upload_img wx-image{ width: 100%; height: 100%; }
.bottom{ margin-bottom: 100rpx; }
.spec{ width: 95%; height: 80rpx; background: #fff; margin: 0 auto; }
.one{ display: inline-block; font-size: 26rpx; }
.one{ vertical-align: top; height: 100%; }
.one>wx-view{ display: inline-block; vertical-align: top; }
.spec>wx-view:nth-child(1){ width: 28%; height: 100%; line-height: 80rpx; margin-left: 5%; }
.spec>wx-view:nth-child(2){ width: 28%; height: 100%; line-height: 80rpx; }
.spec>wx-view:nth-child(3){ width: 28%; height: 100%; line-height: 80rpx; }
.spec>wx-view:nth-child(4){ width: 11%; height: 100%; }
.spec>wx-view wx-image{ width: 50rpx; height: 50rpx; z-index: 5000; }
.one>wx-view:nth-child(1){ width: 60rpx; height: 100%; }
.one>wx-view:nth-child(2){ width: 140rpx; height: 100%; }
.entry{ width: 100%; height: 50rpx; border:1rpx solid #e5e5e5; -webkit-box-sizing: border-box; box-sizing: border-box; }
wx-button{ position: fixed; bottom: 0; width: 100%; height: 80rpx; line-height: 80rpx; border:1rpx solid #ED414A; background: #ED414A; color: #fff; }
